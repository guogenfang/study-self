@charset "utf-8";
body {
	margin: 0;
	padding: 0;
	font-size: 12px;
	font-family:'Microsoft YaHei',regular,"\5FAE\x8F6F\6B63\9ED1\4F53 ","\5FAE\8F6F\96C5\9ED1","\9ED1\4F53","\5b8b\4f53",'Hiragino Sans GB', tahoma, helvetica, "microsoft yahei", arial;
	color: #676767;
}
input,textarea{
		font-family:'Microsoft YaHei',regular,"\5FAE\x8F6F\6B63\9ED1\4F53 ","\5FAE\8F6F\96C5\9ED1","\9ED1\4F53","\5b8b\4f53",'Hiragino Sans GB', tahoma, helvetica, "microsoft yahei", arial;
		color:#676767;
	}

a{text-decoration: none;outline: none;color:#6B7A7F;}
a:visited{text-decoration: none;}
a:hover{cursor: pointer;text-decoration: none;}
a:focus{-moz-outline-style: none;text-decoration: none;}
img{border: 0;}
.clear{clear: both;}

/*---------login_wrapper-----------*/
.login_wrapper{
		position:fixed;
		left:50%;
		top:50%;
		width:479px;
		min-height:375px;
		background: rgba(255,255,255,0.3);
	    -moz-border-radius: 10px;
	    -webkit-border-radius: 10px;
	    border-radius: 10px;
	    -webkit-box-shadow: 0px 0px 0px #777;
	    -moz-box-shadow: 0px 0px 0px #777;
	    box-shadow: 0px 0px 7px #777;
		overflow:hidden;
	}
	.login_wrapper .login_banner{
		box-shadow:2px 2px 4px 0px #abaaaa;
		-webkit-box-shadow:2px 2px 4px 0px #abaaaa;
		-moz-box-shadow:2px 2px 4px 0px #abaaaa;
		-o-box-shadow:2px 2px 4px 0px #abaaaa;
		-ms-box-shadow:2px 2px 4px 0px #abaaaa;
		-moz-border-radius: 10px 10px 0 0;
	    -webkit-border-radius: 10px 10px 0 0;
	    border-radius: 10px 10px 0 0;
		}
	.login_area{
		  background-color:#eff4fa;
		  padding:2px 80px 15px 80px;
		  width:317px;
		  min-height:145px;
		}
	 	.login_input_area{
					height:24px;
					margin-top:15px;
					clear:both;
		}
				.login_input_area span{
						 width:92px;
						 height:24px;
						 line-height:24px;
						 float:left;
						 text-align:right;
						 color:#393837;
						 margin-right:30px;
					}
					.login_input_area span.verification_span{
						  width:70px;
						  float:right;
						  margin-right:0px;
						  text-align:center;
						  border:1px solid #d5d6da;
						  height:22px;
						  line-height:22px;
						  background-color: #EFEFEF;
						  cursor: pointer;
						}
				.login_input_area input{
					height:22px;
					line-height:22px;
					width:193px;
					border:1px solid #d5d6da;
					margin:0px;
					padding:0px;
				}
				.login_input_area input.verification_input{
					    width:118px;
					}
				.login_input_area input::-ms-clear{
					 	display: none;
					 }
				.login_input_area input::-ms-reveal{
						 display: none;
				 	}
		.login_select_area{
				padding-left:74px;
				height:13px;
				line-height:13px;
				margin-top:12px;
				margin-bottom:12px;
				font-size:12px;
				color:#333333;
				vertical-align:middle;
				cursor:default;
			}
				.login_select_area input{
					  	margin-top:0px;
						margin-left:5px;
						margin-bottom:0px;
						margin-right:10px;
						width:13px;
						height:13px;
						padding:0px;
						
					}
				.login_select_area span.auto_login{
						margin-right:5px;
						display:inline;
					}
		 .login_button_wrapper{
			 	height:30px;
				width:100%;
				position:relative;
				left:0px;
				top:0px;
				cursor:default;
			 }
				 .login_button{
						 width:125px;
						 height:30px;
						 display:block;
						 margin:0px auto;
						 background-color:#194a82;
						 border-radius:10px;
						 color:#ffffff;
						 line-height:30px;
						 text-align:center;
					 }
					.lost_password{
							font-size: 12px;
 							color: #b6b5b1;
							position:absolute;
							top:7px;
							left:258px;
						}
